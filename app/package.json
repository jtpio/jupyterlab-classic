    "@jupyterlab-classic/notebook-extension": "^0.1.6",
    "@jupyterlab-classic/terminal-extension": "^0.1.6",
    "@jupyterlab-classic/tree-extension": "^0.1.6",
    "@jupyterlab-classic/ui-components": "^0.1.6",
    "@jupyterlab/apputils-extension": "^3.0.0",
    "@jupyterlab/codemirror-extension": "^3.0.0",
