    "jest": "^26.4.2",
    "mini-css-extract-plugin": "~0.9.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.8.0",
    "raw-loader": "~4.0.0",
    "rimraf": "~3.0.2",
    "style-loader": "~1.0.1",
